{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "target": "es6",
    "types": []
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
